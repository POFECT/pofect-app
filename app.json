{
  "expo": {
    "name": "factory-decision-app",
    "slug": "factory-decision-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/favicon.png",
          "color": "#ffffff"

        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.poscodx.pofectApp",
      "permissions": {
        "notifications": {
          "alert": true,
          "badge": true,
          "sound": false
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.poscodx.pofectApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "a57745ca-b067-4b11-9fdb-4f040bc8997e"
      }
    }
  }
}
